{
  "author": "Mikeal Rogers <mikeal.rogers@gmail.com> (http://www.mikealrogers.com)",
  "name": "framework",
  "description": "A framework for node.js",
  "version": "0.0.0",
  "repository": {
    "url": "git://github.com/mikeal/framework.git"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-es2017": "^6.22.0",
    "coveralls": "^2.12.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "nyc": "^10.1.2",
    "standard": "^9.0.1",
    "tap": "^10.3.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "install": "npm test",
    "pretest": "standard dist",
    "build": "gulp",
    "test": "nyc tap test/*.js --lines=100 --branches=75 --statements=100 --functions=100",
    "posttest": "npm run aftertest",
    "aftertest": "npm run prepublish",
    "postaftertest": "nyc report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "framework": "./dist/bin.js"
  },
  "dependencies": {
    "foreground-child": "^1.5.6"
  },
  "license": "APL-1.0"
}
